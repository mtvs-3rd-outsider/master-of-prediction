<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.outsider.masterofprediction.mapper.AttachmentMapper">


    <select id="getImgById" resultType="java.lang.String">
        SELECT
            attachment_file_address
        FROM
            TBL_ATTACHMENT
        WHERE
            tier_no = #{tierNo}
    </select>
</mapper>
